{
  "name": "@leslies/fe-cli",
  "version": "0.0.6",
  "description": "搭建适合自己的脚手架：实现项目初始化和项目自动化发布两大核心功能。",
  "main": "index.js",
  "bin": {
    "fe": "./bin/index.js"
  },
  "scripts": {
    "dev": "rm -rf lib && rollup -wc rollup.dev.config.js",
    "build": "rm -rf lib && rollup -c rollup.config.js",
    "prepare": "husky install",
    "lint": "lint-staged",
    "test": "npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luozyiii/fe-cli.git"
  },
  "keywords": [
    "fe-cli"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/luozyiii/fe-cli/issues"
  },
  "homepage": "https://github.com/luozyiii/fe-cli#readme",
  "dependencies": {
    "axios": "^0.27.2",
    "colors": "^1.4.0",
    "commander": "^9.4.0",
    "dotenv": "^16.0.1",
    "import-local": "^3.1.0",
    "npmlog": "^6.0.2",
    "root-check": "^2.0.0",
    "semver": "^7.3.7",
    "url-join": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "rollup": "^2.77.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.32.1",
    "typescript": "^4.7.4"
  },
  "lint-staged": {
    "src/**/*.(js|ts)": [
      "npx prettier --write"
    ]
  },
  "files": [
    "bin",
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  }
}
