  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/luozyiii/fe-cli/issues"
  },
  "homepage": "https://github.com/luozyiii/fe-cli#readme",
  "dependencies": {
    "@types/git-clone": "^0.2.0",
    "axios": "^0.27.2",
    "cli-color": "^2.0.3",
    "colors": "^1.4.0",
    "commander": "^9.4.0",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.1",
    "fs-extra": "^10.1.0",
    "git-clone": "^0.2.0",
    "import-local": "^3.1.0",
    "inquirer": "^8.2.4",
    "kebab-case": "^1.0.1",
    "npmlog": "^6.0.2",
    "path-exists": "^5.0.0",
    "root-check": "^2.0.0",
    "semver": "^7.3.7",
    "shelljs": "^0.8.5",
    "ssh2-sftp-client": "^9.0.3",
    "url-join": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "cli-spinner": "^0.2.10",
    "eslint": "^8.20.0",
    "eslint-config-alloy": "^4.6.2",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "npminstall": "^6.5.0",
    "release-it": "^15.4.1",
    "rollup": "^2.77.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "lint-staged": {
    "src/**/*.(js|ts)": [
      "npx prettier --write",
      "eslint --ext .js,.tsx,.ts,.jsx src"
    ]
  },
  "files": [
    "bin",
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  }
}
